me = useTheme();\n\n  const [qty, setQty] = useState(1)\n  const dispatch = useDispatch();\n\n  const productDetails = useSelector(state => state.productDetails);\n  const { loading, product, error } = productDetails;\n\n  useEffect(() => {\n    dispatch(listProductDetails(match.params.id))\n  }, [dispatch, match]);\n\n  const addToCartHandler = () => {\n    history.push(`/cart/${match.params.id}?qty=${qty}`);\n  }\n\n  return (\n    <>\n      {loading ? (\n        <Loader />\n      ) : error ? (\n        <Message severity='error' >{ error }</Message>\n      ) : (\n        <div className=\"productScreen\">\n          <Card className={classes.root}>\n            <CardMedia\n              className={classes.cover}\n              image={product.image}\n              title={product.name}\n            />\n            <div className={classes.details}>\n              <CardContent className={classes.content}>\n                <Typography gutterBottom component=\"h5\" variant=\"h5\">\n                  {product.name}\n                </Typography>\n                <Typography className={classes.typography} gutterBottom variant=\"subtitle2\">\n                  <Rating \n                    value={product.rating} \n                    text={`${product.numReviews} reviews`} \n                  />\n                </Typography>\n                <Typography className={classes.typography} gutterBottom variant=\"h6\">\n                  Price: ${product.price}\n                </Typography>\n                <Divider light />\n                <Typography className={classes.typography} variant=\"body1\" color=\"textSecondary\">\n                {product.description}\n                </Typography>\n              </CardContent>\n            </div>\n          </Card>\n          <Card className={classes.rootAction}>\n            <div className={classes.details}>\n              <CardContent className={classes.content}>\n                <Typography className={classes.typography} gutterBottom variant=\"body1\" color=\"textSecondary\" component=\"p\">\n                  Price: ${product.price}\n                  <Divider light />\n                </Typography>\n                <Typography className={classes.typography} gutterBottom variant=\"body1\" color=\"textSecondary\" component=\"p\">\n                  Status: {product.countInStock > 0 ? 'In Stock' : 'Out of Stock'}\n                  <Divider light />\n                </Typography>\n                {product.countInStock > 0 && (\n                  <FormControl variant=\"outlined\" className={classes.formControl}>\n                    <InputLabel htmlFor=\"age-native-simple\">Qty</InputLabel>\n                    <Select\n                      native\n                      value={qty}\n                      onChange={(e) => setQty(e.target.value)}\n                      inputProps={{\n                        name: 'Qty',\n                        id: 'qty-native-simple',\n                      }}\n                    > \n                      {[...Array(product.countInStock).keys()].map((num) => (\n                        <option key={num + 1} value={num + 1}>{num + 1}</option>\n                      ))}\n                    </Select>\n                  </FormControl>\n                )}\n                <Button \n                  className={classes.button} \n                  variant=\"contained\" \n                  disabled={product.countInStock === 0}\n                  onClick={addToCartHandler}>\n                  Add to Cart\n                </Button>\n              </CardContent>\n            </div>\n          </Card>\n          {/* <Link to='/'>Go Back</Link> */}\n        </div> )}\n    </>\n  )\n}\n\nexport default ProductScreen;\n","/Users/sararussert/codefellows/401/virtual-store/frontend/src/screens/HomeScreen.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/screens/CartScreen.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/components/Foote