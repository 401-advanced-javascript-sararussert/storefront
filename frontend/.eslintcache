re/frontend/src/components/Header.js",["84"],"import React from 'react';\nimport { Link as SiteLink } from 'react-router-dom';\nimport { Breadcrumbs, Link, Typography, Container } from '@material-ui/core';\n\nconst Header = () => {\n  return (\n    <header >\n      <SiteLink to=\"/\" className=\"linkStyle\">\n        <Typography align=\"left\" variant=\"h5\" color=\"textPrimary\" >\n          VIRTUAL STORE\n        </Typography>\n      </SiteLink>\n      <Breadcrumbs aria-label=\"breadcrumb\">\n        <SiteLink to=\"/cart\" className=\"linkStyle\">\n          <Link color=\"inherit\"><i className='fas fa-shopping-cart'></i>\n          CART\n          </Link>\n        </SiteLink>\n        <SiteLink to=\"/login\" className=\"linkStyle\">\n          <Link color=\"inherit\"><i className='fas fa-user'></i>\n          SIGN IN\n          </Link>\n        </SiteLink>\n      </Breadcrumbs>\n    </header>\n  )\n}\n\nexport default Header\n","/Users/sararussert/codefellows/401/virtual-store/frontend/src/components/Footer.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/screens/HomeScreen.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/components/Product.js",["85"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport { makeStyles } from '@material-ui/core/styles';\nimport Card from '@material-ui/core/Card';\nimport CardActionArea from '@material-ui/core/CardActionArea';\nimport CardContent from '@material-ui/core/CardContent';\nimport CardMedia from '@material-ui/core/CardMedia';\nimport Typography from '@material-ui/core/Typography';\nimport Rating from './Rating';\nimport ProductScreen from '../screens/ProductScreen';\n\nconst useStyles = makeStyles({\n  root: {\n    maxWidth: 345,\n  },\n  media: {\n    height: 140,\n  },\n});\n\nconst Product = (props) => {\n  const classes = useStyles();\n  return (\n    <Card className={classes.root}>\n      <CardActionArea>\n        <Link to={`/product/${props.product._id}`}>\n          <CardMedia\n            className={classes.media}\n            image={props.product.image}\n            title={props.product.name}\n          />\n        </Link>\n        <CardContent>\n          <Link to={`/product/${props.product._id}`} className=\"linkStyle\">\n            <Typography gutterBottom variant=\"h6\">\n              {props.product.name}\n            </Typography>\n          </Link>\n          <Typography gutterBottom variant=\"subtitile2\">\n            <Rating \n              value={props.product.rating} \n              text={`${props.product.numReviews} reviews`} \n            />\n          </Typography>\n          <Typography gutterBottom variant=\"subtitle1\">\n            ${props.product.price}\n          </Typography>\n        </CardContent>\n      </CardActionArea>\n    </Card>\n  )\n}\n\nexport default Product\n","/Users/sararussert/codefellows/401/virtual-store/frontend/src/components/Rating.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/screens/ProductScreen.js",["86","87"],"import React, { useState, useEffect } from 'react';\nimport { Link } from 'react-router-dom';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { makeStyles, useTheme } from '@material-ui/core/styles';\nimport Card from '@material-ui/core/Card';\nimport CardContent from '@material-ui/core/CardContent';\nimport CardMedia from '@material-ui/core/CardMedia';\nimport Typography from '@material-ui/core/Typography';\nimport Button from '@material-ui/core/Button';\nimport { Divider } from '@material-ui/core';\nimport InputLabel from '@material-ui/core/InputLabel';\nimport FormControl from '@material-ui/core/FormControl';\nimport Select from '@material-ui/core/Select';\nimport Rating from '../components/Rating';\nimport Loader from '../components/Loader';\nimport Message from '../components/Message';\nimport { listProductDetails} from '../actions/productActions';\n\n\nconst useStyles = makeStyles((theme) => ({\n  root: {\n    display: 'flex',\n    width: 800,\n    height: 500,\n  },\n  rootAction: {\n    display: 'flex',\n    width: 180,\n    height: 230,\n  },\n  details: {\n    display: 'flex',\n    flexDirection: 'column',\n  },\n  content: {\n    flex: '1 0 auto',\n  },\n  cover: {\n    width: 3000,\n  },\n  button: {\n    margin: '10px',\n    backgroundColor: 'black',\n    color: 'white',\n  },\n  formControl: {\n    margin: theme.spacing(1),\n    minWidth: 120,\n  },\n  typography: {\n    marginTop: '10px',\n  }\n  \n\n}));\n\nconst ProductScreen = ({ history, match }) => {\n  const classes = useStyles();\n  const theme = useTheme();\n\n  const [qty, setQty] = useState(1)\n  const dispatch = useDispatch();\n\n  const productDetails = useSelector(state => state.productDetails);\n  const { loading, product, error } = productDetails;\n\n  useEffect(() => {\n    dispatch(listProductDetails(match.params.id))\n  }, [dispatch, match]);\n\n  const addToCartHandler = () => {\n    history.push(`/cart/${match.params.id}?qty=${qty}`);\n  }\n\n  return (\n    <>\n      {loading ? (\n        <Loader />\n      ) : error ? (\n        <Message severity='error' >{ error }</Message>\n      ) : (\n        <div className=\"productScreen\">\n          <Card className={classes.root}>\n            <CardMedia\n              className={classes.cover}\n              image={product.image}\n              title={product.name}\n            />\n            <div className={classes.details}>\n              <CardContent className={classes.content}>\n                <Typography gutterBottom component=\"h5\" variant=\"h5\">\n                  {product.name}\n                </Typography>\n                <Typography className={classes.typography} gutterBottom variant=\"subtitle2\">\n                  <Rating \n                    value={product.rating} \n                    text={`${product.numReviews} reviews`} \n                  />\n                </Typography>\n                <Typography className={classes.typography} gutterBottom variant=\"h6\">\n                  Price: ${product.price}\n                </Typography>\n                <Divider light />\n                <Typography className={classes.typography} variant=\"body1\" color=\"textSecondary\">\n                {product.description}\n                </Typography>\n              </CardContent>\n            </div>\n          </Card>\n          <Card className={classes.rootAction}>\n            <div className={classes.details}>\n              <CardContent className={classes.content}>\n                <Typography className={classes.typography} gutterBottom variant=\"body1\" color=\"textSecondary\" component=\"p\">\n                  Price: ${product.price}\n                  <Divider light />\n                </Typography>\n                <Typography className={classes.typography} gutterBottom variant=\"body1\" color=\"textSecondary\" component=\"p\">\n                  Status: {product.countInStock > 0 ? 'In Stock' : 'Out of Stock'}\n                  <Divider light />\n                </Typography>\n                {product.countInStock > 0 && (\n                  <FormControl variant=\"outlined\" className={classes.formControl}>\n                    <InputLabel htmlFor=\"age-native-simple\">Qty</InputLabel>\n                    <Select\n                      native\n                      value={qty}\n                      onChange={(e) => setQty(e.target.value)}\n                      inputProps={{\n                        name: 'Qty',\n                        id: 'qty-native-simple',\n                      }}\n                    > \n                      {[...Array(product.countInStock).keys()].map((num) => (\n                        <option key={num + 1} value={num + 1}>{num + 1}</option>\n                      ))}\n                    </Select>\n                  </FormControl>\n                )}\n                <Button \n                  className={classes.button} \n                  variant=\"contained\" \n                  disabled={product.countInStock === 0}\n                  onClick={addToCartHandler}>\n                  Add to Cart\n                </Button>\n              </CardContent>\n            </div>\n          </Card>\n          {/* <Link to='/'>Go Back</Link> */}\n        </div> )}\n    </>\n  )\n}\n\nexport default ProductScreen;\n","/Users/sararussert/codefellows/401/virtual-store/frontend/src/store.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/reducers/productReducers.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/constants/productConstants.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/actions/productActions.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/components/Message.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/components/Loader.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/screens/CartScreen.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/reducers/cartReducers.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/constants/cartConstants.js",[],"/Users/sararussert/codefellows/401/virtual-store/frontend/src/actions/cartActions.js",[],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":3,"column":41,"nodeType":"94","messageId":"95","endLine":3,"endColumn":50},{"ruleId":"92","severity":1,"message":"96","line":10,"column":8,"nodeType":"94","messageId":"95","endLine":10,"endColumn":21},{"ruleId":"92","severity":1,"message":"97","line":2,"column":10,"nodeType":"94","messageId":"95","endLine":2,"endColumn":14},{"ruleId":"92","severity":1,"message":"98","line":59,"column":9,"nodeType":"94","messageId":"95","endLine":59,"endColumn":14},"no-native-reassign",["99"],"no-negated-in-lhs",["100"],"no-unused-vars","'Container' is defined but never used.","Identifier","unusedVar","'ProductScreen' is defined but ne